<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:cssClass="aem-Layout aem-Layout--3x3"
        cq:designPath="/libs/settings/wcm/designs/screens"
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/libs/screens/core/templates/splitscreenchannel/lbar-left"
        jcr:primaryType="cq:PageContent"
        jcr:title="Surfing Poster (Left L-Bar)"
        sling:resourceSuperType="screens/core/components/channel"
        sling:resourceType="screens/core/components/multiscreenchannel">
        <r1c1
            cq:cssClass="aem-LayoutCell--2-1"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <image
                jcr:primaryType="nt:unstructured"
                sling:resourceType="screens/core/components/content/image"
                fileReference="/content/dam/we-retail/en/products/activities/surfing/Chasing Tail.jpg"/>
        </r1c1>
        <r1c2
            cq:cssClass="aem-LayoutCell--2-2"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <image
                jcr:primaryType="nt:unstructured"
                sling:resourceType="screens/core/components/content/image"
                fileReference="/content/dam/we-retail/en/experiences/arctic-surfing-in-lofoten/camp-fire.jpg"/>
        </r1c2>
        <r3c1
            cq:cssClass="aem-LayoutCell--1-3 u-aem-Layout-clearLeft"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/text"
                text="&lt;p style=&quot;text-align: center;&quot;>The new surfing catalog is out!&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>Grab it today&lt;/p>&#xa;"
                textIsRich="true"/>
        </r3c1>
    </jcr:content>
</jcr:root>
