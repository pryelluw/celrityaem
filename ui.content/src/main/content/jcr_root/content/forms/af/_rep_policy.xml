<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-users"
        rep:privileges="{Name}[crx:replicate,rep:write]"/>
    <deny1
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[expJson,calcExp,visibleExp,initScript,clickExp,valueCommitScript,enabledExp,validateExp,completionExp,summaryExp,optionsExp,navigationChangeExp]"/>
    </deny1>
    <deny2
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/fd:scripts/*"/>
    </deny2>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-script-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*"
            rep:itemNames="{Name}[expJson,calcExp,visibleExp,initScript,clickExp,valueCommitScript,enabledExp,validateExp,completionExp,summaryExp,optionsExp,navigationChangeExp]"/>
    </allow3>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-script-writers"
        rep:privileges="{Name}[jcr:modifyProperties]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/fd:scripts/*"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="fd-service"
        rep:privileges="{Name}[rep:write]"/>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="cm-agent-users"
        rep:privileges="{Name}[jcr:read]"/>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-submission-reviewers"
        rep:privileges="{Name}[jcr:read]"/>
</jcr:root>
